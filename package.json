{
  "name": "next_js_template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "start": "next start -p 8080",
    "build:prod": "copy .env.prod .env.local && next build && npm run postbuild",
    "build:dev": "copy .env.dev .env.local && next build && npm run postbuild",
    "postbuild": "next-sitemap",
    "tsc": "tsc --p ./tsconfig.json",
    "lint": "next lint --fix",
    "prettier": "prettier -w .",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "precommit": "npm run tsc && npm run prettier && npm run lint && npm run stylelint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "next-intl": "^3.11.3",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "use-debounce": "^10.0.1",
    "yup": "^1.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "stylelint": "^16.6.1",
    "stylelint-config-clean-order": "^6.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5"
  }
}
